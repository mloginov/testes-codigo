{
  "manifest_version": 3,
  "name": "Testes Codigo",
  "description": "Testes Codigo Extension",
  "version": "1.0",
  "action": {
    "default_icon": "favicon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["scripts/questao/index.js"],
      "css": ["scripts/questao/main.css"],
      "matches": [
        "https://testes-codigo.pt/questao/*"
      ]
    }
  ]
}
